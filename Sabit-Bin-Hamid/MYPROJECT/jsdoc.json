{
    "source": {
        "include":["./"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    
    "opts":{
        "encoding":"utf8",
        "verbose":true,
        "recurse":true,
        "destination":"docs/"
    }
    
}