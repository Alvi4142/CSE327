<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= data.pageTitle%></title>
    <link rel="stylesheet" href="/css/style.css" type="text/css">
</head>
<body>
   

    
    <div class="main">
        <div class="navbar">
            <h2 class="logo">MediSolution</h2>       
        </div> 

        <%- include("./partial/navbar.ejs") %> 
        <div  class="Review">
            <table>
                <thead>
                    <tr class="p">
                        <th>Name</th>

                        <th>Email</th>

                        <th>Review</th>
                    
                    </tr>
                </thead>
                <tbody>
                    <% data.viewReview.forEach(s => {  %>
                        <tr>
                            <td><%= s.Name %></td>
                            <td><%= s.Email %></td>
                            <td><%= s.Review %></td>
                            
                        </tr>
                        <% })%>
                </tbody>

                <div id= 'container'>
                    
                </div>
            </table>
        </div>
    </div>
</body>
</html>